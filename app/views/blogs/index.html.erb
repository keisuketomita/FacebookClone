<h1>投稿一覧</h1>
<p><%= notice %></p>
<table class="center-block">
  <tr>
    <th>投稿内容</th>
    <th>ユーザー</th>
  </tr>
  <% @blogs.each do |blog| %>
    <tr>
      <td><%= link_to blog.content,edit_blog_path(blog.id)  %></td>
      <td><%= link_to blog.user.name,user_path(blog.user.id)  %></td>
    </tr>
  <% end %>
</table>
